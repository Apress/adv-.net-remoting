<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.runtime.remoting>
		<application>
			<channels>
				<channel ref="tcp">
					<clientProviders>
						<formatter ref="binary" />
						<provider type="Microsoft.Samples.Runtime.Remoting.Security.SecurityClientChannelSinkProvider, Microsoft.Samples.Runtime.Remoting.Security" 
									securityPackage="negotiate" 
									impersonationLevel="identify"
									authenticationLevel="packetPrivacy" />
					</clientProviders>
				</channel>
			</channels>
			<client>
				<wellknown	type="General.IPersonFactory, General"
									url="tcp://localhost:1234/MyPersonManager.rem" />
			</client>
		</application>
	</system.runtime.remoting>
</configuration>
